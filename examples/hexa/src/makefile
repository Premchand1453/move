####### Compiler, tools and options

CC	=	gcc
CXX	=	g++
CFLAGS	=	-pipe -finline -Winline
CXXFLAGS=	-pipe -finline -Winline -LANG:std
INCPATH	=	-I../../..
LINK	=	g++
LFLAGS	=	-LANG:std
LIBS	=	$(SUBLIBS) 
MOC	=	$(QTDIR)/bin/moc
UIC	=	$(QTDIR)/bin/uic

TAR	=	tar -cf
GZIP	=	gzip -9f

####### Files

